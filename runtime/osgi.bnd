Bundle-Activator:\
  ${bundle.namespace}.internal.Activator

Bundle-Version>:\
  ${pom.version}

Bundle-SymbolicName:\
  ${bundle.symbolicName}

Embed-Dependency:\
 *;scope=runtime;inline=true

Export-Package: \
  org.ops4j.pax.exam.*,\
  junit.*,\
  org.easymock.*,\
  org.hamcrest.*,\
  org.junit.*

Private-Package:\
  org.ops4j.lang.*

Import-Package:\
  !aQute.lib.osgi.*,*;resolution:=optional

DynamicImport-Package:\
  *

Include-Resource:\
  META-INF=src/main/resources/META-INF/
